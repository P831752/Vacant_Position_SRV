{
  "name": "Vacant_Position_SRV",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^4.1.2",
    "@sap-cloud-sdk/resilience": "^4.1.2",
    "@sap/cds": "^9",
    "express": "^4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "auth": "dummy"
      },
      "destinations": true,
      "sf_dest": {
        "kind": "rest",
        "credentials": {
          "destination": "SF_DEST",
          "requestTimeout": 30000,
          "forwardAuthToken": true
        }
      }
    }
  }
}
